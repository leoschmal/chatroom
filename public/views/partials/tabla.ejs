<% if(productos.length !== 0) { %>
    <div class="table-responsive">
        <table class="table table-dark">
            <tr style="color: yellow;"> <th>Producto</th> <th>Precio</th> <th>Imagen</th> </tr>
           <% for (const producto in productos ) { %>
            <tr>
                <td> <%= productos[producto].title %> </td>
                <td> <%= productos[producto].price %> </td>
                <td> <img src= <%= productos[producto].url %> alt=<%= productos[producto].title %> width="30" height="30"></td>
            </tr>
            <% } %>
        </table>
    </div>
<% } else{ %> 
   <h3 style="color: black">No hay Productos</h3>
<% }%>

